<h1>Sign Up</h1>

<form action="<%= users_url %>" method= "post">
    <%= auth_token %>
    <label> Email: 
        <input name="user[email]">
    </label>
    <label> Username: 
        <input name="user[username]">
    </label>
    <label> Password: 
        <input type="password" name="user[password]">
    </label>

    <input type="submit" value="Sign Up">
</form>